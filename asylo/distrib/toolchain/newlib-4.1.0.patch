+
+void _exit(int rc) {
+  enclave_exit(rc);
+}
+
+int _close(int fildes) {
+  uint64_t args[] = {(uint64_t)fildes};
+  return enclave_syscall(SYS_close, args, 1);
+}
+
+int _execve(const char *name, char *const argv[], char *const env[]) {
+  uint64_t args[] = {(uint64_t)name,
+                   (uint64_t)argv,
+                   (uint64_t)env};
